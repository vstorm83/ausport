        "ListTrackings": {
            "httpMethod": "GET",
            "uri": "QueryTracking.json",
            "parameters": {
                "tracking_id": {
                    "required": true,
                    "type": "string",
                    "location": "query",
                    "sentAs": "q"
                }
            }
        },
        "ValidateAddress": {
            "httpMethod": "GET",
            "uri": "ValidateAddress.json",
            "parameters": {
                "address_line_1": {
                    "required": true,
                    "type": "string",
                    "location": "query",
                    "sentAs": "addressLine1"
                },
                "address_line_2": {
                    "required": false,
                    "type": "string",
                    "location": "query",
                    "sentAs": "addressLine2"
                },
                "state": {
                    "required": true,
                    "type": "string",
                    "location": "query"
                },
                "suburb": {
                    "required": true,
                    "type": "string",
                    "location": "query"
                },
                "postcode": {
                    "required": true,
                    "type": "string",
                    "location": "query"
                },
                "country": {
                    "required": true,
                    "type": "string",
                    "location": "query"
                }
            }
        }
    }
}
